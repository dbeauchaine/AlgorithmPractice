{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": false,
    "max-line-length": [ true, 200 ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "prefer-for-of": false,
    "no-console": false,
    "object-literal-sort-keys": false

  }
}